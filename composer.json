{
    "name": "mingalevme/illuminate-lock",
    "description": "Laravel/Lumen decorator fot symfony/lock component",
    "keywords": [
        "laravel",
        "lumen",
        "lock",
        "mutex",
        "semaphore"
    ],
    "homepage": "https://github.com/mingalevme/illuminate-lock",
    "license": "MIT",
    "authors": [
        {
            "name": "Mingalev Mikhail",
            "email": "mingalevme@gmail.com"
        }
    ],
    "require": {
        "symfony/lock": "^4.0"
    },
    "require-dev": {
        "laravel/lumen": "^5.0",
        "laravel/laravel": "^5.0",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "^1.0",
        "predis/predis": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Mingalevme\\Illuminate\\Lock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mingalevme\\Tests\\Illuminate\\Lock\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
